n    const breakpoints = [0];\n    let sum = 0;\n    imagesWidths.forEach(item => {\n      const iterationSum = sum += item;\n      breakpoints.push(iterationSum);      \n    })\n    \n    setImageBreakpoints(breakpoints)\n  }, [carouselScrollWidth, imagesWidths])\n\n\n  useEffect(() => {\n    const newPosition = imageBreakpoints[count - 1];\n    if (newPosition || newPosition === 0) {\n      // newPosition !== sliderPosition && setSliderPosition(newPosition)\n    }\n  }, [count, imageBreakpoints])\n\n\n  // Event handlers\n  const handleCarouselScroll = value => {\n    if (sliderRef?.current?.valueAsNumber) {\n      sliderRef.current.valueAsNumber = value\n    }\n  }\n\n\n  const handleSliderChange = e => {\n    setSliderPosition(Number(e.target.value))\n  }\n\n\n  const handleArrowClick = arrowType => {\n    let counter = count;\n    if (arrowType === 'increase' && count < imagesWidths.length) counter++;\n    else if (arrowType === 'decrease' && count > 1) counter--;\n    setCount(counter)\n  }\n\n\n  return (\n    <motion.nav\n      initial={{ opacity: 0 }}\n      animate={{ opacity: 1 }}\n      transition={{ duration: 0.25 }}\n    >\n      <s.Home>\n      {/* Vertical box with photo count */}\n      <s.VerticalCounterWrapper darkTheme={darkTheme}>\n        <s.VerticalCounter palette={palette} darkTheme={darkTheme}>\n          <p>{count}</p>\n          <p>{imagesWidths.length}</p>\n        </s.VerticalCounter>\n      </s.VerticalCounterWrapper>\n\n        <s.CarouselContainer>\n          {/* Main view */}\n          <CarouselWithScrollbar \n            setCarouselScrollWidth={setCarouselScrollWidth}\n            setImagesWidth={setImagesWidth}\n            handleCarouselScroll={handleCarouselScroll}\n            sliderPosition={sliderPosition}\n          />\n\n          {/* Footer with slider */}\n          <s.BottomSliderWrapper darkTheme={darkTheme}>\n            <s.CarouselButtonsWrapper palette={palette}>\n              <FontAwesomeIcon icon={faCaretLeft} onClick={() => handleArrowClick(\"decrease\")} />              \n              <FontAwesomeIcon icon={faCaretRight} onClick={() => handleArrowClick(\"increase\")} />\n            </s.CarouselButtonsWrapper>\n            \n            <s.CarouselSliderWrapper>\n              <s.Slider\n                type='range'\n                min={1}\n                max={carouselScrollWidth}\n                ref={sliderRef}\n                palette={palette}\n                onChange={e => handleSliderChange(e)}\n              />\n            </s.CarouselSliderWrapper>\n          </s.BottomSliderWrapper>\n        </s.CarouselContainer>\n        {/* <Footer /> */}\n      </s.Home>\n    </motion.nav>\n  );\n}\n\nexport default Home","/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/About/About.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Home/Home.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/Shared/Carousel/Carousel.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/Shared/Carousel/Carousel.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/Shared/ScreenSlider/ScreenSlider.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/Shared/ScreenSlider/ScreenSlider.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Portfolio/Portfolio.js",["117","118"],"import { useState, useEffect, useContext } from 'react'\nimport * as s from './Portfolio.style'\nimport { motion } from 'framer-motion'\n\n// Components\nimport TwoColumnCarousel from './TwoColumnCarousel'\n\n// Context\nimport AppContext from 'Context';\n\n\nconst Portfolio = () => {\n    // Context\n    const context = useContext(AppContext);\n    const { darkTheme, palette } = context;\n\n    return (\n        <motion.nav\n          initial={{ opacity: 0 }}\n          animate={{ opacity: 1 }}\n          transition={{ duration: 0.25 }}\n        >\n            <s.Portfolio>\n                {/* Vertical box with photo count */}\n                <s.VerticalCounterWrapper darkTheme={darkTheme}>\n                    <s.VerticalCounter palette={palette} darkTheme={darkTheme}>\n                        <p>1</p>\n                        <p>40</p>\n                    </s.VerticalCounter>\n                </s.VerticalCounterWrapper>\n\n                {/* Two column carousel */}\n                <s.TwoColumnCarouselWrapper>\n                    <TwoColumnCarousel />\n                </s.TwoColumnCarouselWrapper>\n\n                {/* Footer with slider */}\n                <s.BottomSliderWrapper darkTheme={darkTheme}>\n                    Footer\n                </s.BottomSliderWrapper>\n            </s.Portfolio>\n        </motion.nav>\n    )\n}\n\nexport default Portfolio","/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Portfolio/Portfolio.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Prints/Prints.js",[],["119","120"],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Prints/Prints.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Contact/Contact.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Contact/Contact.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/Shared/BlackButton/BlackButton.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Portfolio/TwoColumnCarousel.js",["121"],"/Users/eugenecostov/Documents/Code/marc-eyme/src/components/MainView/Portfolio/TwoColumnCarousel.style.js",[],"/Users/eugenecostov/Documents/Code/marc-eyme/src/Galleries.js",[],{"ruleId":"122","replacedBy":"123"},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","severity":1,"message":"127","line":20,"column":9,"nodeType":"128","messageId":"129","endLine":20,"endColumn":16},{"ruleId":"126","severity":1,"message":"130","line":1,"column":10,"nodeType":"128","messageId":"129